    <validationRules>
        <fullName>Age_Validation_on_Customer_DOB</fullName>
        <active>true</active>
        <description>Customers with Age below 18 cannot create record.</description>
        <errorConditionFormula>AND( ISNEW() , (YEAR(TODAY()) - YEAR(DOB__c)) &lt; 18 )</errorConditionFormula>
        <errorMessage>Customer&apos;s DOB should be &gt;= 18</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Date_of_Joining_Validation</fullName>
        <active>true</active>
        <description>DOJ can be today or Grater than today.</description>
        <errorConditionFormula>DOJ__c  &lt; TODAY()</errorConditionFormula>
        <errorDisplayField>DOJ__c</errorDisplayField>
        <errorMessage>Date of Joining Cannot be Less than today.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Make_Subscription_Type_as_Blank</fullName>
        <active>true</active>
        <description>To make sure we are blanking out the subscription Type to blank once the customer goes inactive.</description>
        <errorConditionFormula>AND(PRIORVALUE( IsActive__c ),NOT( ISPICKVAL(Subscription_Type__c, &apos;&apos;)), IsActive__c == false)</errorConditionFormula>
        <errorMessage>Please reset the Subscription Type to blank.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Making_Subscription_Type_Mandatory</fullName>
        <active>true</active>
        <description>Make the Subscription Type as Mandatory when the Customer is Active</description>
        <errorConditionFormula>AND ( IsActive__c  ,ISPICKVAL( Subscription_Type__c , &apos;&apos; ))</errorConditionFormula>
        <errorMessage>Please choose a Value in Subscription Type.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Mandate_Reactivation_Date</fullName>
        <active>true</active>
        <errorConditionFormula>AND ( ISCHANGED( Customer_Status__c ) ,  ISPICKVAL(Customer_Status__c , &apos;Suspended from Subscription&apos;) , ISBLANK( Re_Activate_After_Suspension__c ) )</errorConditionFormula>
        <errorMessage>Please Enter the Re Activation Date for Suspended Customer</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Pin_code_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>if(
     TEXT(Country__c) == &apos;US&apos;,
            NOT(REGEX(Pincode__c, &quot;\\d{5}(-\\d{4})?&quot;)),
     if(TEXT(Country__c) == &apos;India&apos;,
            NOT(REGEX(Pincode__c, &quot;\\d{6}&quot;)),
     if(TEXT(Country__c) == &apos;Canada&apos;,
            NOT(REGEX(Pincode__c,
	&quot;((?i)[ABCEGHJKLMNPRSTVXY]\\d[A-Z]?\\s?\\d[A-Z]\\d)?&quot;)),
false))
)</errorConditionFormula>
